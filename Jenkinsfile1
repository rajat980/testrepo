pipeline {
    agent any
    environment {
        AWS_CREDS = credentials('major')  // ID of the AWS credentials you created
    }
    stages {
        stage('Deploy') {
            steps {
                withCredentials([aws(credentialsId: 'AWS_CREDS', region: 'us-east-1')]) {
                    script {
                        sh 'aws s3 ls'
                    }
                }
            }
        }
    }
}
