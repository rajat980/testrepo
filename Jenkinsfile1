pipeline {
    agent any
    stages {
        stage('Use AWS') {
            steps {
                
                  sh '''
                    aws --version
                    aws ec2 describe-internet-gateways
                    aws ec2 describe-key-pairs
                    aws cloudformation create-stack --stack-name ${STACK_NAME} --template-body file://cft/ec2.yml --region 'us-east-1'
                    ''' 
'
                
            }
        }
    }
}
